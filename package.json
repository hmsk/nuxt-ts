{
  "name": "nuxt-ts",
  "version": "0.0.2",
  "description": "TypeScript module for Nuxt 2+",
  "license": "MIT",
  "contributors": [
    {
      "name": "hmsk <k.hamasaki@gmail.com>"
    }
  ],
  "keywords": [
    "nuxt",
    "vue",
    "typescript",
    "webpack"
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/hmsk/nuxt-ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push origin master --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {},
  "peerDependencies": {
    "ts-loader": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "nuxt": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "standard-version": "latest"
  }
}
